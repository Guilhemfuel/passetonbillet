{
    "name": "PTB/PasseTonBillet",
    "description": "L'application web du site PasseTonBillet.",
    "keywords": ["ticket", "PasseTonBillet","train","travel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://www.setasign.com/downloads/"
        }
    ],
    "require": {
        "php": ">=7",
        "ext-bcmath": "*",
        "ext-gd": "*",
        "algolia/algoliasearch-client-php": "^2.2",
        "anhskohbo/no-captcha": "^3.0",
        "aws/aws-sdk-php": "^3.112",
        "barryvdh/laravel-debugbar": "^3.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~4.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "graham-campbell/manager": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "imangazaliev/didom": "^1.13",
        "intervention/image": "^2.4",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^6.0",
        "laravel/passport": "^7.0",
        "laravel/scout": "^7.2.1",
        "laravel/socialite": "^4.2.0",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "~1.0",
        "league/csv": "^9.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "nexmo/laravel": "^2.0",
        "nicolaslopezj/searchable": "1.12.0",
        "pusher/pusher-php-server": "~3.0",
        "sentry/sentry-laravel": "^1.0",
        "setasign/fpdf": "1.8.1",
        "setasign/fpdi": "^2.0",
        "setasign/fpdi_pdf-parser": "2.0.4",
        "spatie/laravel-cookie-consent": "^2.4",
        "spatie/laravel-sitemap": "^5.3",
        "tightenco/ziggy": "^0.8.0",
        "torann/geoip": "^1.0",
        "vinkla/hashids": "^7.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.2",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^5.5.2",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    }
}
